<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<bean id="someBean" class="ioc.ioc3.SomeBean3">
		<property name="setprops">
			<set>
				<value>机构</value>
				<value>柜台</value>
				<value>员工</value>
				<value>业务</value>
			</set>
		</property>
		<property name="listprops">
			<list>
				<value>branch</value>
				<value>counter</value>
				<value>stuff</value>
				<value>operation</value>
			</list>
		</property>
		<property name="mapprops">
			<map>
				<entry key="机构">
					<value>北京市朝阳区酒仙桥分行</value>
				</entry>
				<entry key="柜台">
					<value>1号柜台</value>
				</entry>
				<entry key="员工">
					<value>郑晓健</value>
				</entry>
				<entry key="业务">
					<value>对公业务</value>
				</entry>
			</map>
		</property>
		<property name="props">
			<props>
				<prop key="数据库驱动">MySQL Driver</prop>
				<prop key="用户名">root</prop>
				<prop key="密码">123456789</prop>
			</props>
		</property>
	</bean>

</beans>